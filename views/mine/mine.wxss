.view-mine .recommond{}
.view-mine .recommond .header{
	display:flex;
	height:80rpx;
	justify-content:space-between;
	align-items:flex-start;
	border-bottom:1rpx solid #e5e5e5;
}
.view-mine .recommond .header .title{
	font-size:16px;
	color:#202020;
}
.view-mine .recommond .content{
	margin-top: 34rpx;
	display:flex;
flex-wrap:wrap;
}
.view-mine .tpl-recommond-ware{
	width:331rpx;
	display:flex;
	flex-direction:column;
	align-items:center;
}
.view-mine .tpl-recommond-ware .top{}
.view-mine .tpl-recommond-ware .top .ware-img{
	width:300rpx;
	height:300rpx;
}
.view-mine .tpl-recommond-ware .bottom{
	display:flex;
	flex-direction:column;
	height:200rpx;
	width:300rpx;
	position: relative;
}
.view-mine .tpl-recommond-ware .bottom .ware-name{
	font-size:14px;
	color:#232323;
	height:25px;
	line-height:25px;
}
.view-mine .tpl-recommond-ware .bottom .feature{
	font-size:13px;
	color:#969696;
	height:25px;
	line-height:25px;
}
.view-mine .tpl-recommond-ware .bottom .price-info{}
.view-mine .tpl-recommond-ware .bottom .price-info .new-price:before,
.view-mine .tpl-recommond-ware .bottom .price-info .old-price:before{
	content:'ï¿¥';
}
.view-mine .tpl-recommond-ware .bottom .price-info .new-price{
	color:#d24740;
	font-size: 14px;
	margin-right: 10rpx;
}
.view-mine .tpl-recommond-ware .bottom .price-info .old-price{
	color:#969696;
	font-size: 12px;
	text-decoration: line-through;
}
.view-mine .tpl-recommond-ware .bottom .addCart{
	position:absolute;
	top:110rpx;
	right:0;
}
.view-mine .tpl-recommond-ware .bottom .addCart .cartimg{
	width: 50rpx;
	height: 50rpx
}


.view-mine .goLogin-wrapper{
	position:relative;
	height:100%;
	display:flex;
	flex-direction:column;
	justify-content:center;
	align-items:center;
	/*background-color: #F8F8F8*/

}
.view-mine .goLogin-wrapper .login-logo{
	width:150rpx;
	height:160rpx;
	margin-bottom:140rpx;
}
.view-mine .goLogin-wrapper .login-msg{
	font-size:16px;
	color:#999999;
	margin-bottom:112rpx;
}
.view-mine .goLogin-wrapper .login-btn{
	font-size:17px;
	color:#FFFFFF;
	background:#d6204b;
	box-shadow:0 5px 10px 0 rgba(0,0,0,0.20);
	border-radius:80px;
	width:320rpx;
	height:100rpx;
	display:flex;
	justify-content:center;
	align-items:center;
	margin-bottom:200rpx;
}