	font-size:16px;
	color:#202020;
}
.recommond .content{
	margin-top: 34rpx;
	display:flex;
flex-wrap:wrap;
}
.tpl-recommond-ware{
	width:331rpx;
	display:flex;
	flex-direction:column;
	align-items:center;
}
.tpl-recommond-ware .top{}
.tpl-recommond-ware .top .ware-img{
	width:300rpx;
	height:300rpx;
}
.tpl-recommond-ware .bottom{
	display:flex;
	flex-direction:column;
	height:200rpx;
	width:300rpx;
	position: relative;
}
.tpl-recommond-ware .bottom .ware-name{
	font-size:14px;
	color:#232323;
	height:25px;
	line-height:25px;
}
.tpl-recommond-ware .bottom .feature{
	font-size:13px;
	color:#969696;
	height:25px;
	line-height:25px;
}
.tpl-recommond-ware .bottom .price-info{}
.tpl-recommond-ware .bottom .price-info .new-price:before,
.tpl-recommond-ware .bottom .price-info .old-price:before{
	content:'ï¿¥';
}
.tpl-recommond-ware .bottom .price-info .new-price{
	color:#d24740;
	font-size: 14px;
	margin-right: 10rpx;
}
.tpl-recommond-ware .bottom .price-info .old-price{
	color:#969696;
	font-size: 12px;
	text-decoration: line-through;
}
.tpl-recommond-ware .bottom .addCart{
	position:absolute;
	top:110rpx;
	right:0;
}
.tpl-recommond-ware .bottom .addCart .cartimg{
	width: 50rpx;
	height: 50rpx
}