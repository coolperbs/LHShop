
.view-mine{
	background-color: #eeeff1;
	height: 100%
}

.view-mine .pannel{
	margin-bottom:20rpx;
	background-color: white;
	padding: 44rpx;
}

.view-mine .user{
	display:flex;
height:140rpx;
justify-content:space-between;
/*padding:44rpx;*/
}
.view-mine .user .left{}
.view-mine .user .left .name{
	font-size:24px;

}
.view-mine .user .left .notice{
	height: 17px;
	line-height: 17px;
}
.view-mine .user .left .check-user{
	font-size:12px;
color:#a6a6a6;
height:14px;
line-height:14px
}
.view-mine .user .right .header-img{
	width:140rpx;
height:140rpx;
border-radius:50%;
}


.view-mine .order{}
.view-mine .order .header{
display:flex;
height:80rpx;
justify-content:space-between;
align-items:flex-start;
border-bottom:1rpx solid #e5e5e5;
}
.view-mine .order .header .title{
	font-size:16px;
color:#202020;
}
.view-mine .order .header .check-more{
	font-size:12px;
color:#9e9e9e;
}

.view-mine .order .content{
	display:flex;
justify-content:space-between;
margin-top: 34rpx;
}
.view-mine .order .content .item{
	width:165rpx;
	display:flex;
	flex-direction:column;
	align-items:center;

}
.view-mine .order .content .item .item-logo{
	width: 50rpx;
	height: 50rpx;
}
.view-mine .order .content .item .item-txt{
	font-size: 13px;
	color:#1d1d1d;
	margin-top: 12rpx;
	color: #313131
}


.view-mine .my-service{}
