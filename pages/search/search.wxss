@import '../../views/mine/mine.wxss'
.search-content{
	margin-top: 34rpx;
	display:flex;
	flex-wrap:wrap;
	padding: 44rpx;
	margin-top: 80rpx
}
.search-box{
	height:80rpx;
	z-index:100;
	border-bottom:solid 1px #f8f8f8;
	background-color:#fff;
	position:fixed;
	top:1px;
	left:0;
	width:100%;
	display:flex;
	align-items:center;
	justify-content:center;
}

.search-box input{
	width:550rpx;
background-color:#f8f8f8;
border-radius:150rpx;
padding-left:50rpx;
font-size:13px;
display:flex;
}
.search-box image{
	width:30rpx;
height:30rpx;
position:absolute;
z-index:10;
left:85rpx;
}